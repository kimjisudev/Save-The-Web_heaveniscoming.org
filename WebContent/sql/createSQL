CREATE TABLE `gallery` (
	`gno` INT(10) NOT NULL AUTO_INCREMENT,
	`gtitle` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`gupfolder` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`guuid` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`gfilename` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`gdate` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (`gno`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=40
;


CREATE TABLE `news` (
	`nno` INT(10) NOT NULL AUTO_INCREMENT,
	`ntitle` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`nupfolder` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`nuuid` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`nfilename` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`ndate` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (`nno`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=13
;


CREATE TABLE `post` (
	`pno` INT(10) NOT NULL AUTO_INCREMENT,
	`ptitle` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`pupfolder` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`puuid` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`pfilename` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`ppage` INT(10) NULL DEFAULT NULL,
	`pdate` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (`pno`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=13
;
